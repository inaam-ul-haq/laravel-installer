{
    "name": "inaam-ul-haq/installer",
    "description": "Laravel web installer",
    "license": "MIT",
    "version": "1.1",
    "authors": [
        {
            "name": "Inaam ul haq",
            "email": "waleedhaq339@gmail.com"
        }
    ],
    "require": {
        "php": ">=7.0.0"
    },
    "autoload": {
        "psr-4": {
            "Inaam\\Installer\\": "src/"
        },
        "files": [
            "src/Helpers/functions.php"
        ]
    },
    "extra": {
        "laravel": {
            "providers": [
                "Inaam\\LaravelInstaller\\Providers\\LaravelInstallerServiceProvider"
            ]
        }
    },
    "config": {
        "sort-packages": true
    },
    "minimum-stability": "stable"
}